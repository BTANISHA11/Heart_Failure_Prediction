{
    "version": 2,
    "builds": [
      {
        "src": "src/index.html",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*).js",
        "dest": "/src/$1.js"
      },
      {
        "src": "/(.*).css",
        "dest": "/src/$1.css"
      },
      {
        "src": "/(.*).(jpg|jpeg|png|gif|ico)",
        "dest": "/src/images/$1.$2"
      },
      {
        "src": "/favicon.ico",
        "dest": "/src/favicon.ico"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/src/index.html"
      }
    ]
  }
  
  